<h1>Create new Interview</h1>

<%= form_for @interview do |f| %>
    <%= f.collection_select(:applicant_id, @applicants, :id, :name, prompt: "Please select an Applicant") %>
    <%= f.datetime_field :date %>
    <%= f.hidden_field :interviewer_id, value: @interviewer.id %>
    <%= f.submit %>
<% end %>

